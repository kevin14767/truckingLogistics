{
  "expo": {
    "name": "truckingLogistics",
    "slug": "truckingLogistics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.kevin14767.truckingLogistics",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kevin14767.truckingLogistics",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.109641224107-uf20r2grrc2nq7p4bopl1o7djqc18rnk"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kevin14767.truckingLogistics"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.109641224107-uf20r2grrc2nq7p4bopl1o7djqc18rnk"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
        
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload receipts.",
          "cameraPermission": "Allow Truck Logistics Pro to access your camera"
        }
      ]

    ],    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "anthropicApiKey": "process.env.ANTHROPIC_API_KEY",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e51bc337-3d5d-4e99-a19f-96fce61917de"
      }
      
    }
  }
}